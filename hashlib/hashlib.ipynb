   "source": [
    "import hashlib as hl\n",
    "\n",
    "test_str = 'hello world'\n",
    "\n",
    "for algo in (hl.sha1, hl.sha256, hl.sha512):\n",
    "    m = algo()\n",
    "    m.update(test_str.encode())\n",
    "    print(f'{algo.__name__:15} {m.hexdigest()}')"
   ]
